{
  "name": "marleyjs",
  "version": "0.7.4",
  "engines": {
    "node": ">=8.11.2",
    "npm": ">=5.6.0"
  },
  "private": true,
  "repository": {},
  "description": "Modern Angular 6+ Stack.",
  "scripts": {
    "ng": "ng",
    "test": "jest --watch",
    "lint": "tslint ./src/**/*.ts -t verbose",
    "lint:styles": "stylelint ./src/**/*.scss",
    "lint:ci": "yarn lint:styles && yarn lint",
    "conventional-changelog": "conventional-changelog",
    "e2e": "ng e2e",
    "e2e:ci": "testcafe all e2e/**/*.e2e-spec.ts --app 'yarn start' --app-init-delay 5000",
    "test:ci": "jest --runInBand",
    "test:coverage": "rimraf coverage && jest --coverage",
    "analyzer": "ng build --prod --stats-json && webpack-bundle-analyzer dist/stats.json",
    "prettier": "prettier {src,e2e}/*.ts --write && prettier {src,e2e}/**/*.ts --write",
    "setup:project": "yarn install && yarn build:ssr && yarn start",
    "compodoc": "./node_modules/.bin/compodoc -p src/tsconfig.app.json -n \"Marley.js documentation\" -w -o --theme \"postmark\"",
    "compodoc:serve": "./node_modules/.bin/compodoc -p src/tsconfig.app.json -s -n \"Marley.js documentation\" -w -o --theme \"postmark\"",
    "build:client:server": "yarn lint:styles && yarn prettier && yarn build:prod && ng run angular.io-example:server",
    "webpack:server": "webpack --config webpack.server.config.js --progress --colors",
    "build:ssr": "yarn build:client:server && yarn webpack:server && node version.js",
    "build:dev": "ng build",
    "build:prod": "ng build --prod",
    "serve": "ng serve",
    "start": "node dist/server.js"
  },
  "author": "Petr Nikolas",
  "license": "MIT",
  "dependencies": {
    "@angular/animations": "^6.0.0",
    "@angular/common": "^6.0.0",
    "@angular/compiler": "^6.0.0",
    "@angular/core": "^6.0.0",
    "@angular/forms": "^6.0.0",
    "@angular/http": "^6.0.0",
    "@angular/platform-browser": "^6.0.0",
    "@angular/platform-browser-dynamic": "^6.0.0",
    "@angular/router": "^6.0.0",
    "@angular/upgrade": "^6.0.0",
    "@nguniversal/express-engine": "^6.0.0",
    "@nguniversal/module-map-ngfactory-loader": "^6.0.0",
    "@nicky-lenaers/ngx-scroll-to": "^1.1.1",
    "angular-in-memory-web-api": "^0.6.0",
    "bulma": "^0.7.1",
    "core-js": "^2.5.4",
    "git-describe": "^4.0.3",
    "jquery": "^3.3.1",
    "leaflet": "^1.3.4",
    "ngx-facebook": "^2.4.0",
    "ngx-slick-carousel": "^0.4.0",
    "normalize.css": "^8.0.0",
    "rxjs": "^6.0.0",
    "slick-carousel": "^1.8.1",
    "zone.js": "^0.8.24"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.6.0",
    "@angular/cli": "^6.0.0",
    "@angular/compiler-cli": "^6.0.0",
    "@angular/platform-server": "^6.0.0",
    "@compodoc/compodoc": "^1.1.3",
    "@types/jasmine": "~2.8.0",
    "@types/jasminewd2": "^2.0.3",
    "@types/node": "^6.0.45",
    "conventional-changelog-cli": "^2.0.1",
    "describe": "^1.2.0",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "jest": "^23.4.2",
    "jest-axe": "^3.0.0",
    "jest-image-snapshot": "^2.4.3",
    "jest-preset-angular": "^5.2.3",
    "jest-sonar-reporter": "^2.0.0",
    "karma": "^3.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~1.4.2",
    "karma-jasmine": "~1.1.1",
    "karma-jasmine-html-reporter": "^0.2.2",
    "karma-phantomjs-launcher": "^1.0.2",
    "lodash": "^4.16.2",
    "phantomjs-prebuilt": "^2.1.7",
    "prettier": "^1.13.7",
    "protractor": "^5.4.0",
    "rimraf": "^2.6.2",
    "sass-lint": "^1.12.1",
    "stylelint": "^9.5.0",
    "stylelint-config-css-modules": "^1.3.0",
    "testcafe": "^0.20.5",
    "testcafe-live": "^0.1.3",
    "ts-loader": "^4.2.0",
    "ts-node": "^5.0.1",
    "tslint": "^5.9.1",
    "tslint-eslint-rules": "^5.3.1",
    "tslint-language-service": "^0.9.9",
    "typescript": "2.7.2",
    "webpack-bundle-analyzer": "^2.13.1",
    "webpack-cli": "^2.0.14"
  }
}
